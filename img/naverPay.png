//사업자정보확인
function openBusinessInfo(){
	/* $("#layerPopupBusinessInfo").find("#bizinfo_content").load("http://www.ftc.go.kr/bizCommPop.do?wrkr_no=2148609977&apv_perm_no=2011322016230200086",function(response, status, xhr){
