function goAction(obj) {
	
	var url = "";  
    $("#lnbDspCtgryForm").find('#dspCtgryNo').val("");
    $("#lnbDspCtgryForm").find('#currentCtgryDpthCd').val("");
    $("#lnbDspCtgryForm").find('#ctgrySectCd').val("");
	if($(obj).attr("id") =='PROMOTION'){
		   url = "/event/promotionList";
	}else if($(obj).attr("id") =='STYLE'){
		 url = "/style/styleList";
	}else if($(obj).attr("id") =='BESTREVIEW'){
		 url = "/display/reviewsList";
	}else if($(obj).attr("id") =='DISCOVERER' || $(obj).attr("id") =='DISCOVERER_Stories'){
		 url = "/discoverer/discovererList";
	}else if($(obj).attr("id") =='DISCOVERER_Videos'){
		 url = "/discoverer/discovererVideoList";
	}else{
  
	    var ctgryNo = $(obj).attr("ctgryNo");
	    var currentCtgryDpthCd = $(obj).attr("currentCtgryDpthCd");
	    var ctgrySectCd = $(obj).attr("ctgrySectCd");
	    url = "/display/view";
	    $("#lnbDspCtgryForm").find('#dspCtgryNo').val(ctgryNo);
	    $("#lnbDspCtgryForm").find('#currentCtgryDpthCd').val(currentCtgryDpthCd);
	    $("#lnbDspCtgryForm").find('#ctgrySectCd').val(ctgrySectCd);
	 
	    if(currentCtgryDpthCd == '1'){
	        url = "/display/majorView";
	        $("#lnbDspCtgryForm").find('#ctgryNoDpth1').val(ctgryNo);
	        $("#lnbDspCtgryForm").find('#ctgryNoDpth2').val("");
	        $("#lnbDspCtgryForm").find('#ctgryNoDpth3').val("");
	    	 
	    }else if(currentCtgryDpthCd == '2'){
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth1').val($(obj).attr("ctgryNoDpth1"));
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth2').val(ctgryNo);
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth3').val("");
	    }else if(currentCtgryDpthCd == '3'){
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth1').val($(obj).attr("ctgryNoDpth1"));
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth2').val($(obj).attr("ctgryNoDpth2"));
	    	 $("#lnbDspCtgryForm").find('#ctgryNoDpth3').val(ctgryNo);
	    }

	}

    $('#realForm').attr('action', PATH_LOCALE + url);

    $("#lnbDspCtgryForm").find('input').each(function(i) {     	
    	if($(this).val() !=''){
    		$('#realForm').append($(this));
    	} 	
    });

    var frm = $('#realForm');
 
	frm.submit();
}


